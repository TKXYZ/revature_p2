<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<display-name>GatherUp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<display-name>PersonRepoServlet</display-name>
		<servlet-name>PersonRepoServlet</servlet-name>
		<servlet-class>com.gatherup.servlet.PersonRepoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PersonRepoServlet</servlet-name>
		<url-pattern>/PersonRepoServlet</url-pattern>
	</servlet-mapping>
</web-app>
